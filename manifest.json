{
    "name": "TextJoy",
    "version": "1.0",
    "description": "Generate emoji for every page visited based on the content",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icons/emoji/png/025-thinking.png"
    }
}